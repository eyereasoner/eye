    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo 5];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo ((| "go" "loc3" |) (| "push" "loc2" |) (| "push" "loc1" |) "climb_on" "grab")];
