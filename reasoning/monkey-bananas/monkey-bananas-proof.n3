skolem:lemma5 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            ("consult" "./monkey-bananas.pl") log:program true.
            ("between" 1 5 var:x_0) log:program true.
            ("length" var:x_1 var:x_0) log:program true.
            ("plan" var:x_1) log:program true.
        } => {
            var:x_1 a :Solution.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/monkey-bananas/monkey-bananas-query.n3>].

