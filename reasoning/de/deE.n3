PREFIX : <http://josd.github.io/eye/reasoning#>
PREFIX r: <http://www.w3.org/2000/10/swap/reason#>
PREFIX skolem: <http://josd.github.io/.well-known/genid/nvP8fFxOxACPrFaRcJSML4XR498#>

[] a r:Proof, r:Conjunction;
  r:component <#lemma1>;
  r:gives {
    skolem:e_anon_1 :saying :C.
  }.

<#lemma1> a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :C.
  };
  r:evidence (
    <#lemma2>
  );
  r:rule <#lemma3>.

<#lemma2> a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :C.
  };
  r:evidence (
    <#lemma4>
  );
  r:rule <#lemma5>.

<#lemma3> a r:Extraction;
  r:gives {
    {?x_0_1 :saying :C} => {?x_0_1 :saying :C}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/deQ.n3>].

<#lemma4> a r:Inference;
  r:gives {
    skolem:e_anon_1 :saying :A.
  };
  r:evidence (
    <#lemma6>
  );
  r:rule <#lemma7>.

<#lemma5> a r:Extraction;
  r:gives {
    {?x_0_2 :saying :A} => {?x_0_2 :saying :C}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

<#lemma6> a r:Inference;
  r:gives {
    skolem:e_anon_1 :not_saying :B.
  };
  r:evidence (
    <#lemma8>
  );
  r:rule <#lemma9>.

<#lemma7> a r:Extraction;
  r:gives {
    {?x_0_3 :not_saying :B} => {?x_0_3 :saying :A}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

<#lemma8> a r:Extraction;
  r:gives {
    skolem:e_anon_1 :not_saying :C.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/deA.n3>].

<#lemma9> a r:Extraction;
  r:gives {
    {?x_0_4 :not_saying :C} => {?x_0_4 :not_saying :B}.
  };
  r:because [ a r:Parsing; r:source <http://josd.github.io/eye/reasoning/de/de.n3>].

