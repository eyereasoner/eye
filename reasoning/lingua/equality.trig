# ----------------------
# Graph equality example
# ----------------------

PREFIX log: <http://www.w3.org/2000/10/swap/log#>
PREFIX var: <http://www.w3.org/2000/10/swap/var#>
PREFIX : <#>

GRAPH _:bng_1 {
    :A :B :C.
    :D :E _:bng_2.
}

GRAPH _:bng_2 {
    :F :G var:H.
}

GRAPH _:bng_3 {
    :D :E _:bng_4.
    :A :B :C.
}

GRAPH _:bng_4 {
    :F :G var:I.
}

#query
_:bng_5 log:query _:bng_6.

GRAPH _:bng_5 {
    _:bng_1 log:equalTo _:bng_3.
}

GRAPH _:bng_6 {
    :test :is true.
}
