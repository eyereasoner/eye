#!/bin/bash
eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/ccd/train_data.n3 http://josd.github.io/eye/reasoning/ccd/background_rules.n3 http://josd.github.io/eye/reasoning/ccd/train_rules.n3 --query http://josd.github.io/eye/reasoning/ccd/train_query.n3 --output train_model.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/ccd/train_data.n3 http://josd.github.io/eye/reasoning/ccd/background_rules.n3 http://josd.github.io/eye/reasoning/ccd/train_rules.n3 --query http://josd.github.io/eye/reasoning/ccd/train_query.n3 --output train_model_proof.n3

eye "$@" --wcache http://josd.github.io/eye/reasoning .. --nope http://josd.github.io/eye/reasoning/ccd/test_data.n3 http://josd.github.io/eye/reasoning/ccd/train_model.n3 http://josd.github.io/eye/reasoning/ccd/background_rules.n3 http://josd.github.io/eye/reasoning/ccd/ccd_rules.n3 --query http://josd.github.io/eye/reasoning/ccd/test_query.n3 --output test_answer.n3
eye "$@" --wcache http://josd.github.io/eye/reasoning .. http://josd.github.io/eye/reasoning/ccd/test_data.n3 http://josd.github.io/eye/reasoning/ccd/train_model.n3 http://josd.github.io/eye/reasoning/ccd/background_rules.n3 http://josd.github.io/eye/reasoning/ccd/ccd_rules.n3 --query http://josd.github.io/eye/reasoning/ccd/test_query.n3 --output test_proof.n3
