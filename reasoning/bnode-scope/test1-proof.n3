@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/>.
@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:gives {
        {
            (?U_5 {
                :a :b ?U_6.
            } (:o2)) log:collectAllIn ?U_7.
        } => {
            :a :n :m.
        }.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        {
            (?U_5 {
                :a :b ?U_6.
            } (:o2)) log:collectAllIn ?U_7.
        } => {
            :a :n :m.
        }.
    };
    r:evidence (
        skolem:lemma2
        [ a r:Fact; r:gives {(| "\+" {
            {
                (_:sk_5 {
                    :a :b _:sk_6.
                } (:o2)) log:collectAllIn _:sk_7.
            } log:equalTo true.
        } |)}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        (_:sk_5 {
            :a :b _:sk_6.
        } (:o2)) log:collectAllIn _:sk_7.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :a :n :m.
    }];
    r:rule skolem:lemma3.

skolem:lemma2 a r:Inference;
    r:gives {
        @forAll var:x_11, var:x_12, var:x_13. {
            (var:x_11 {
                :a :b var:x_12.
            } (:o2)) log:collectAllIn var:x_13.
        } => {
            :a :n :m.
        }.
    };
    r:evidence (
        skolem:lemma4
        [ a r:Fact; r:gives {(_:sk_4 {
            :s :p2 _:sk_4.
        } (:o2)) log:collectAllIn ((<https://eyereasoner.github.io/eye/reasoning/bnode-scope/test1.n3>) 1)}]
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/s"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_3"]; r:boundTo ((<https://eyereasoner.github.io/eye/reasoning/bnode-scope/test1.n3>) 1)];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ a r:Existential; n3:nodeId "_:sk_4"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_2"]; r:boundTo (:o2)];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_6"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#x_13"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_4"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#x_11"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_5"]; r:boundTo [ n3:uri "http://www.w3.org/2000/10/swap/var#x_12"]];
    r:rule skolem:lemma5.

skolem:lemma3 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 => var:x_1.
            (| "\+" {
                var:x_0 log:equalTo true.
            } |).
        } => {
            var:x_0 => var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <http://eulersharp.sourceforge.net/2003/03swap/pass-all>].

skolem:lemma4 a r:Extraction;
    r:gives {
        :s :p :o.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/bnode-scope/test1.n3>].

skolem:lemma5 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1, var:x_2, var:x_3. @forSome var:x_4, var:x_5, var:x_6. {
            var:x_0 :p :o.
            (var:x_1 {
                var:x_0 :p2 var:x_1.
            } var:x_2) log:collectAllIn var:x_3.
        } => {
            {
                (var:x_4 {
                    :a :b var:x_5.
                } var:x_2) log:collectAllIn var:x_6.
            } => {
                :a :n :m.
            }.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/reasoning/bnode-scope/test1.n3>].

