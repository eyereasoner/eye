@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <#>.

# beetle is a car
:beetle a :Car.

# all cars are green or blue
(_:A) log:nand {
    _:A a :Car.
    () log:nand {
        _:A :is :green.
    }.
    () log:nand {
        _:A :is :blue.
    }.
}.

# green things are nice or pretty
(_:A) log:nand {
    _:A :is :green.
    () log:nand {
        _:A :is :nice.
    }.
    () log:nand {
        _:A :is :pretty.
    }.
}.

# pretty things are pretty 1 or pretty 2
(_:A) log:nand {
    _:A :is :pretty.
    () log:nand {
        _:A :is :pretty1.
    }.
    () log:nand {
        _:A :is :pretty2.
    }.
}.

# nice things are nice 1 or nice 2
(_:A) log:nand {
    _:A :is :nice.
    () log:nand {
        _:A :is :nice1.
    }.
    () log:nand {
        _:A :is :nice2.
    }.
}.

# pretty 1 things are pretty 11 or pretty 12
(_:A) log:nand {
    _:A :is :pretty1.
    () log:nand {
        _:A :is :pretty11.
    }.
    () log:nand {
        _:A :is :pretty12.
    }.
}.

# pretty 2 things are pretty 21 or pretty 22
(_:A) log:nand {
    _:A :is :pretty2.
    () log:nand {
        _:A :is :pretty21.
    }.
    () log:nand {
        _:A :is :pretty22.
    }.
}.

# nice 1 things are nice 11 or nice 12
(_:A) log:nand {
    _:A :is :nice1.
    () log:nand {
        _:A :is :nice11.
    }.
    () log:nand {
        _:A :is :nice12.
    }.
}.

# nice 2 things are nice 21 or nice 22
(_:A) log:nand {
    _:A :is :nice2.
    () log:nand {
        _:A :is :nice21.
    }.
    () log:nand {
        _:A :is :nice22.
    }.
}.

# pretty or nice or blue things are beautiful
(_:A) log:nand {
    () log:nand {
        () log:nand {
            _:A :is :pretty11.
        }.
        () log:nand {
            _:A :is :pretty12.
        }.
        () log:nand {
            _:A :is :pretty21.
        }.
        () log:nand {
            _:A :is :pretty22.
        }.
        () log:nand {
            _:A :is :nice11.
        }.
        () log:nand {
            _:A :is :nice12.
        }.
        () log:nand {
            _:A :is :nice21.
        }.
        () log:nand {
            _:A :is :nice22.
        }.
        () log:nand {
            _:A :is :blue.
        }.
    }.
    () log:nand {
        _:A :is :beautiful.
    }.
}.

# query
(_:S _:O) log:nand {
    _:S :is _:O.
    () log:nans {
        _:S :is _:O.
    }.
}.
