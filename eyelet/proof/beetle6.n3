@prefix skolem: <https://eyereasoner.github.io/.well-known/genid/8b98b360-9a70-4845-b52c-c675af60ad01#>.
@prefix r: <http://www.w3.org/2000/10/swap/reason#>.
@prefix : <http://example.org/#>.
@prefix n3: <http://www.w3.org/2004/06/rei#>.
@prefix var: <http://www.w3.org/2000/10/swap/var#>.
@prefix list: <http://www.w3.org/2000/10/swap/list#>.

skolem:proof a r:Proof, r:Conjunction;
    r:component skolem:lemma1;
    r:component skolem:lemma2;
    r:gives {
        :beetle :is :green.
        :beetle :is :nice.
    }.

skolem:lemma1 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#green"]];
    r:rule skolem:lemma4.

skolem:lemma2 a r:Inference;
    r:gives {
        :beetle :is :nice.
    };
    r:evidence (
        skolem:lemma5
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo [ n3:uri "http://example.org/#nice"]];
    r:rule skolem:lemma4.

skolem:lemma3 a r:Inference;
    r:gives {
        :beetle :is :green.
    };
    r:evidence (
        skolem:lemma6
        skolem:lemma7
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        {
            var:x_0 :is :blue.
        } => ($ $).
        var:x_0 a :Car.
    } => {
        var:x_0 :is :green.
    }}].

skolem:lemma4 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 :is var:x_1.
        } => {
            var:x_0 :is var:x_1.
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/eyelet/input/beetle6.n3>].

skolem:lemma5 a r:Inference;
    r:gives {
        :beetle :is :nice.
    };
    r:evidence (
        skolem:lemma8
        skolem:lemma3
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule [ a r:Fact; r:gives {@forSome var:x_0. {
        {
            var:x_0 :is :pretty.
        } => ($ $).
        var:x_0 :is :green.
    } => {
        var:x_0 :is :nice.
    }}].

skolem:lemma6 a r:Inference;
    r:gives {
        {
            :beetle :is :blue.
        } => ($ $).
    };
    r:evidence (
        [ a r:Fact; r:gives {{
            :beetle :is :blue.
        } => {
            :beetle :is :beautiful.
        }}]
        [ a r:Fact; r:gives {{
            :beetle :is :beautiful.
        } list:isList false}]
        skolem:lemma9
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :beetle :is :blue.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :beetle :is :beautiful.
    }];
    r:rule skolem:lemma10.

skolem:lemma7 a r:Extraction;
    r:gives {
        :beetle a :Car.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/eyelet/input/beetle6.n3>].

skolem:lemma8 a r:Inference;
    r:gives {
        {
            :beetle :is :pretty.
        } => ($ $).
    };
    r:evidence (
        [ a r:Fact; r:gives {{
            :beetle :is :pretty.
        } => {
            :beetle :is :beautiful.
        }}]
        [ a r:Fact; r:gives {{
            :beetle :is :beautiful.
        } list:isList false}]
        skolem:lemma9
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo {
        :beetle :is :pretty.
    }];
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_1"]; r:boundTo {
        :beetle :is :beautiful.
    }];
    r:rule skolem:lemma10.

skolem:lemma9 a r:Inference;
    r:gives {
        {
            :beetle :is :beautiful.
        } => ($ $).
    };
    r:evidence (
        skolem:lemma7
    );
    r:binding [ r:variable [ n3:uri "http://www.w3.org/2000/10/swap/var#x_0"]; r:boundTo [ n3:uri "http://example.org/#beetle"]];
    r:rule skolem:lemma11.

skolem:lemma10 a r:Extraction;
    r:gives {
        @forAll var:x_0, var:x_1. {
            var:x_0 => var:x_1.
            var:x_1 list:isList false.
            var:x_1 => ($ $).
        } => {
            var:x_0 => ($ $).
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/eyelet/eyelet.n3>].

skolem:lemma11 a r:Extraction;
    r:gives {
        @forAll var:x_0. {
            var:x_0 a :Car.
        } => {
            {
                var:x_0 :is :beautiful.
            } => ($ $).
        }.
    };
    r:because [ a r:Parsing; r:source <https://eyereasoner.github.io/eye/eyelet/input/beetle6.n3>].

