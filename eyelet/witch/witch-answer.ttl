@prefix : <http://example.org/#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix proof: <http://www.w3.org/2000/10/swap/reason#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix var: <http://www.w3.org/2000/10/swap/var#> .

:GIRL a :WITCH .

[] proof:produced ( [ log:triple ( :DUCK rdf:type :ISMADEOFWOOD ) ] ) ;
    proof:used ( [ log:triple ( :DUCK rdf:type :FLOATS ) ] ) ;
    proof:viaRule ( [ log:triple ( [ log:graph ( [ log:triple ( var:x rdf:type :FLOATS ) ] ) ] log:implies [ log:graph ( [ log:triple ( var:x rdf:type :ISMADEOFWOOD ) ] ) ] ) ] ) .

[] proof:produced ( [ log:triple ( :GIRL rdf:type :FLOATS ) ] ) ;
    proof:used ( [ log:triple ( :DUCK rdf:type :FLOATS ) ] [ log:triple ( :DUCK :SAMEWEIGHT :GIRL ) ] ) ;
    proof:viaRule ( [ log:triple ( [ log:graph ( [ log:triple ( var:x rdf:type :FLOATS ) ] [ log:triple ( var:x :SAMEWEIGHT var:y ) ] ) ] log:implies [ log:graph ( [ log:triple ( var:y rdf:type :FLOATS ) ] ) ] ) ] ) .

[] proof:produced ( [ log:triple ( :DUCK rdf:type :BURNS ) ] ) ;
    proof:used ( [ log:triple ( :DUCK rdf:type :ISMADEOFWOOD ) ] ) ;
    proof:viaRule ( [ log:triple ( [ log:graph ( [ log:triple ( var:x rdf:type :ISMADEOFWOOD ) ] ) ] log:implies [ log:graph ( [ log:triple ( var:x rdf:type :BURNS ) ] ) ] ) ] ) .

[] proof:produced ( [ log:triple ( :GIRL rdf:type :WITCH ) ] ) ;
    proof:used ( [ log:triple ( :GIRL rdf:type :BURNS ) ] [ log:triple ( :GIRL rdf:type :WOMAN ) ] ) ;
    proof:viaRule ( [ log:triple ( [ log:graph ( [ log:triple ( var:x rdf:type :BURNS ) ] [ log:triple ( var:x rdf:type :WOMAN ) ] ) ] log:implies [ log:graph ( [ log:triple ( var:x rdf:type :WITCH ) ] ) ] ) ] ) .

